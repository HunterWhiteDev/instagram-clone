{
  "name": "new-instagram",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "restart": "vite",
    "dev": "npm run up",
    "up": "concurrently \"supabase start\" \"npm run db:seed-storage\" \"vite\"",
    "db:seed-storage": "ts-node supabase/scripts/seed-storage.ts",
    "db:reset": "supabase db reset && npm run db:seed-storage",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "gen-types": "supabase gen types typescript --local > types/supabase.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@supabase/supabase-js": "^2.39.2",
    "@tailwindcss/vite": "^4.1.4",
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.14.1",
    "@types/tailwindcss": "^3.0.11",
    "css-to-tailwind-translator": "^1.2.8",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "match-sorter": "^6.3.1",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "sort-by": "^1.2.0",
    "tailwindcss": "^4.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
